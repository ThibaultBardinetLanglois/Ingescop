{% extends 'base.html.twig' %}
{% block pageContainer %}

    <div class="client-service-page">

        <h1>Nos services</h1>
        <div class="services-list">
            {% for service in services %}
                <a href="{{ path('services_show', {id: service.id}) }}" class="bubble-pro"><p>{{ service.name }}</p></a>
            {% endfor %}
        </div>
    </div>

{% endblock %}